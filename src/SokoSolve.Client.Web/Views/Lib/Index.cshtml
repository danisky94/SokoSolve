@model SokoSolve.Core.Lib.LibraryCollection

@{
    ViewBag.Title = "Library Collection";
}

<h2>@ViewBag.Title</h2>

<ul>
    @foreach (var i in Model.Items)
    {
        <li><a asp-action="Home" asp-route-id="@i.Id">@i.Name</a> <a asp-action="Solutions" asp-route-id="@i.Id" class="btn btn-info btn-sm">Solutions</a></li>
    }
</ul>
